<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="api">
                    <match url="*/*" />
                    <action type="Rewrite" url="http://www.lumosvue.com/{R:2}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
